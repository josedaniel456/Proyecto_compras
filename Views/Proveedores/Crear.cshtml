@{
    Layout = null;
}
@model Proyecto_compras.Models.Proveedor

<h2>Nuevo Proveedor</h2>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"]</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body class="container">
<form method="post">
    <div class="mb-3">
        <label>Razón Social</label>
        <input asp-for="razon_social" class="form-control" />
    </div>
    <div class="mb-3">
        <label>RUC</label>
        <input asp-for="ruc" class="form-control" />
    </div>
    <div class="mb-3">
        <label>Correo</label>
        <input asp-for="correo" class="form-control" />
    </div>
    <div class="mb-3">
        <label>Celular</label>
        <input asp-for="celular" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
    <a href="/Proveedores" class="btn btn-secondary">Cancelar</a>
</form>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>